<% articles.map(function(item){%>
    <div class="media">
  <div class="media-left">
    <a href="/article?id=<%= item.id %>">
      <img class="media-object" src="images/<%= item.preImage %>" alt="...">
    </a>
  </div>
  <div class="media-body">
    <h4 class="media-heading"><%= item.title %></h4>
   <%= item.description %>
   </div>
</div>

<%});%> 
