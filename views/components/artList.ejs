<% articles.map(function(item){%>
   

<article id="post-hello-world" class="article article-type-post animated slideInDown" itemscope="" itemprop="blogPost">
  
    <div class="article-meta">
  	<time class="article-date" datetime="2015-11-04T09:25:11.000Z" itemprop="datePublished"><span class="glyphicon glyphicon-calendar" aria-hidden="true"></span> <%= item.createDate %> <%= item.createTime %></time>

    </div>
  
  <div class="article-inner">
    
      <input type="hidden" class="isFancy">
    
    
      <header class="article-header">
        
  
    <h1 itemprop="name">
      <a class="article-title" href="/2015/11/04/hello-world/"><%= item.title %></a>
    </h1>
    <p class="article-info">From:<%- item.author %> / Tags:<%- item.tags %></p>

      </header>
      
    
    <div class="article-entry" itemprop="articleBody">
      
       <div class="markdown-body">
       
      <%- item.content %>
    </div>
      
    </div>
    
    <div class="article-info article-info-index">
      <div class="clearfix"></div>
    </div>
    
  </div>
  
</article>

<%});%> 
